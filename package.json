{
    "name": "@shopfront/bridge",
    "version": "2.0.7",
    "main": "./lib/index.js",
    "license": "ISC",
    "description": "The bridge used to embed your application within Shopfront",
    "homepage": "https://developer.onshopfront.com/documentation/Embedded/Introduction",
    "bugs": "https://github.com/onshopfront/shopfront-embedded-bridge/issues",
    "repository": {
        "type": "git",
        "url": "https://github.com/onshopfront/shopfront-embedded-bridge.git"
    },
    "author": "Shopfront <api@shopfront.com.au> (https://shopfront.com.au)",
    "contributors": [
        "Nicholas Clark",
        "Adam Simpkins",
        "Zachary Stanford"
    ],
    "type": "module",
    "private": false,
    "devDependencies": {
        "@auto-it/all-contributors": "^11.3.0",
        "@auto-it/jira": "^11.3.0",
        "@onshopfront/core": "^0.1.97",
        "auto": "^11.3.0",
        "husky": "^9.1.7",
        "ts-jest-resolver": "^2.0.1",
        "typescript": "^5.9.3"
    },
    "scripts": {
        "type-check": "tsc --noEmit",
        "type-check:watch": "npm run-script type-check --watch",
        "build": "tsc",
        "watch": "tsc -w",
        "lint": "npx core lint .",
        "lint:commit:run": "npx core lint-commit",
        "lint:pr:run": "npx core lint-pr --target-branch origin/master",
        "test": "npx @onshopfront/core test"
    },
    "auto": {
        "plugins": [
            "npm",
            "all-contributors",
            [
                "jira",
                "https://onshopfront.atlassian.net"
            ],
            "released"
        ],
        "author": {
            "name": "Auto Release",
            "email": "nick@shopfront.com.au"
        }
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org",
        "access": "public"
    }
